<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleTimerWidget">
        <!-- Widget color       -->
        <attr name="widget_color" format="color" />
        <!-- Left-top widget color       -->
        <attr name="widget_color_start" format="color" />
        <!-- Right-bottom widget color       -->
        <attr name="widget_color_end" format="color" />
        <!-- Inner start color       -->
        <attr name="widget_color_center" format="color" />
        <!-- Inner end color       -->
        <attr name="widget_color_edge" format="color" />
        <!-- Circle width       -->
        <attr name="circle_width" format="dimension" />
        <!-- Misfires count     -->
        <attr name="separators_count" format="integer" />
        <!-- Length of misfire  -->
        <attr name="separator_length" format="dimension" />
        <!-- Indicator drawable -->
        <attr name="indicator_drawable" format="reference" />
    </declare-styleable>

</resources>